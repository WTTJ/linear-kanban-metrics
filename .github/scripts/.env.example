# GitHub Configuration (Required)
GITHUB_REPOSITORY=your-org/your-repo
GITHUB_TOKEN=your_github_token
PR_NUMBER=123

# API Provider Selection (Required)
# Choose either "anthropic" or "dust"
API_PROVIDER=anthropic

# Anthropic API Configuration (Required when API_PROVIDER=anthropic)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Dust API Configuration (Required when API_PROVIDER=dust)
DUST_API_KEY=your_dust_api_key
DUST_WORKSPACE_ID=your_dust_workspace_id
DUST_AGENT_ID=your_dust_agent_configuration_id

# Optional: Enable debug logging
DEBUG=true
